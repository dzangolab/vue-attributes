{
  "name": "@dzangolab/vue-attributes",
  "version": "0.1.0",
  "description": "Attributes components for vue .js (vue3)",
  "keywords": [
    "vuejs",
    "attributes"
  ],
  "homepage": "https://github.com/dzangolab/vue-attributes",
  "bugs": "https://github.com/dzangolab/vue-attributes/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dzangolab/vue-attributes.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dzango Technologies Limited",
    "email": "info@dzango.com",
    "url": "https://www.dzangolab.com"
  },
  "main": "./dist/attributes.common.js",
  "files": [
    "*.js",
    "*.json",
    "*.ts",
    "dist/*",
    "lib/*",
    "public/*"
  ],
  "scripts": {
    "build": "vue-cli-service build --target lib --name attributes ./lib/index.js",
    "commitlint": "commitlint --edit $1",
    "lint": "vue-cli-service lint",
    "package-sort": "npx sort-package-json",
    "prepare": "husky install",
    "prepublishOnly": "yarn run sort-package && yarn run lint && yarn run test && yarn run build",
    "serve": "vue-cli-service serve",
    "test": "yarn run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "release": "shipjs prepare"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.14",
    "@vue/cli-service": "^4.5.14",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.0",
    "babel-jest": "^26.0",
    "bootstrap": "^5.1",
    "core-js": "^3.6.5",
    "eslint": "^7.32, < 8.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "7.0.4",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0, < 11",
    "shipjs": "0.24.0",
    "vue": "^3.2.22",
    "vue-jest": "^5.0.0-0"
  },
  "peerDependencies": {
    "vue": ">= 3.0"
  },
  "engines": {
    "node": ">= 14"
  }
}
